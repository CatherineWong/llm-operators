{"pour": {"operator_name": "pour", "pddl_operator": "(:action pour\n        :parameters (?l - food ?c - container)\n        :precondition (and\n            (pourable ?l)\n        )\n        :effect (and\n            (placedon ?l ?c)\n            (forall\n                (?d - container)\n                (when\n                    (not (= ?d ?c))\n                    (not (placedon ?l ?d))\n                )\n            )\n        )\n    )"}, "spread": {"operator_name": "spread", "pddl_operator": "(:action spread\n        :parameters (?j - food ?b - food ?k - knife)\n        :precondition (and\n            ; (isclean ?k)\n            (spreadable ?j)\n        )\n        :effect (and\n            (spreadon ?j ?b)\n            (not (isclean ?k))\n        )\n    )"}, "put": {"operator_name": "put", "pddl_operator": "(:action put\n        :parameters (?f - food ?c - container)\n        :precondition ()\n        :effect (and\n            (placedon ?f ?c)\n            (forall\n                (?d - container)\n                (when\n                    (not (= ?d ?c))\n                    (not (placedon ?f ?d))\n                )\n            )\n        )\n    )"}, "serve": {"operator_name": "serve", "pddl_operator": "(:action serve\n        :parameters (?c - container)\n        :precondition ()\n        :effect (and\n            (served ?c)\n        )\n    )"}, "toast": {"operator_name": "toast", "pddl_operator": "(:action toast\n        :parameters (?f - food)\n        :precondition (and\n            (toastable ?f)\n        )\n        :effect (and\n            (toasted ?f)\n        )\n    )"}, "fry": {"operator_name": "fry", "pddl_operator": "(:action fry\n        :parameters (?f - food)\n        :precondition (and\n            (fryable ?f)\n        )\n        :effect (and\n            (fried ?f)\n        )\n    )"}}